{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "APK Downloader"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "Direct download APK file and install the app manually onto your Android devices.",
   "icons": {
      "128": "images/icon-128x128.png",
      "32": "images/favicon@2x.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "APK Downloader",
   "permissions": [ "https://apk.support/*", "https://*.apk.support/*", "https://*.androidcontents.com/*", "https://androidcontents.com/*", "https://www.google.com/", "https://*.google.com/*", "https://*.googleapis.com/*", "http://www.google.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.9"
}
